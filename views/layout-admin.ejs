<!DOCTYPE html>
<html ng-app="myStandAdminApp">
  <head>
    <title>My Stand Admin</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <meta name="fragment" content="!" />
    <base href="/">

    <link href='//fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700' rel='stylesheet' type='text/css'>
    <!--STYLES-->
    <link rel="stylesheet" href="/styles/admin/importer.css">
    <link rel="stylesheet" href="/bower_components/textAngular/src/textAngular.css">
    <!--STYLES END-->
    <script type="text/javascript">
      window.SAILS_LOCALS = { _csrf: '<%= _csrf %>' };
    </script>
  </head>
  <body>

    <ng-include src="'assets/templates/admin/shared/header.html'"></ng-include>

    <section class="ms-admin-center">
      <div class="row">
        <div class="large-12 columns ms-admin-center-inner">
          <div class="row">
            <div class="large-3 columns no-pad-left no-pad-right">
              <div class="ms-admin-menu-wrapper">
                <ng-include src="'assets/templates/admin/shared/menu.html'"></ng-include>
              </div>
            </div>

            <div class="large-9 columns no-pad-left no-pad-right">
              <div class="ms-admin-content">

                <ng-view autoscroll="true"></ng-view>

              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <ng-include src="'assets/templates/admin/shared/footer.html'"></ng-include>

    <!-- DO NOT REMOVE OR CHANGE THESE COMMENTS -- >
    <!--TEMPLATES-->
    <script type="text/javascript" src="/admin/jst.js"></script>
    <!--TEMPLATES END-->

    <!-- DO NOT REMOVE OR CHANGE THESE COMMENTS -- >
    <!--SCRIPTS-->
    <script src="/bower_components/angular/angular.min.js"></script>
    <script src="/bower_components/angular-route/angular-route.min.js"></script>
    <script src="/bower_components/angular-resource/angular-resource.min.js"></script>
    <script src="/bower_components/lodash/lodash.min.js"></script>
    <script src="/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="/bower_components/foundation/js/foundation.min.js"></script>
    <script src="/bower_components/textAngular/dist/textAngular-rangy.min.js"></script>
    <script src="/bower_components/textAngular/dist/textAngular-sanitize.min.js"></script>
    <script src="/bower_components/textAngular/dist/textAngular.min.js"></script>
    <script src="/js/admin/app.js"></script>
    <script src="/js/admin/current-user/service.js"></script>
    <script src="/js/admin/home/controller.js"></script>
    <script src="/js/admin/rest-service/service.js"></script>
    <script src="/js/admin/routes.js"></script>
    <script src="/js/admin/users/controller.js"></script>
    <!--SCRIPTS END-->
  </body>
</html>
